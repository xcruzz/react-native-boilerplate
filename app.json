{
  "expo": {
    "name": "Lugaro 2020",
    "slug": "lugaro2020",
    "privacy": "public",
    "platforms": ["android"],
    "version": "8.11.02",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splashAL.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "permissions": [],
      "versionCode": 8,
      "backgroundColor": "#000000",
      "icon": "./assets/icon.png",
      "splash": {
        "backgroundColor": "#000000"
      },
      "package": "com.alexandralugaro2020.app"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "svg", "ts", "tsx"]
    },
    "description": "A través de esta aplicación podrás repasar las biografías de candidatos a posiciones principales, alcaldías, legislatura y legislaturas municipales del MVC. Todos comprometidos con la Agenda Urgente. "
  }
}
